# Minimal BusyBox for initramfs (aarch64)
CONFIG_STATIC=y
CONFIG_INSTALL_APPLET_SYMLINKS=y

CONFIG_FEATURE_SH_STANDALONE=y
CONFIG_ASH=y
CONFIG_SH_IS_ASH=y
CONFIG_CTTYHACK=y

# core utils
CONFIG_CAT=y
CONFIG_ECHO=y
CONFIG_LS=y
CONFIG_MKDIR=y
CONFIG_MKNOD=y
CONFIG_MOUNT=y
CONFIG_UMOUNT=y
CONFIG_DMESG=y
CONFIG_UNAME=y
CONFIG_PWD=y
CONFIG_PS=y
CONFIG_SLEEP=y

# power control
CONFIG_POWEROFF=y
CONFIG_REBOOT=y
CONFIG_HALT=y
